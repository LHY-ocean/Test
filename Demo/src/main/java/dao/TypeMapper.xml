<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

 <mapper namespace="dao.TypeDao2">
 
 	<select id="select" resultType="model.Type" >
 		select * from type;
 	</select>
 	
 	<insert id="insert" parameterType="model.Type">
 		insert into type(name) values(#{name})
 	</insert>
 	
 	<delete id="delete" parameterType="int">
 		delete from type where id=#{_parameter}
 	</delete>
 	
 	<update id="update" parameterType="model.Type">
 		update type set name=#{name} where id=#{id}
 	</update>
 	
 </mapper>